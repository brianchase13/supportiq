{
  "name": "supportiq",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered support analytics that cuts ticket costs by 30%",
  "keywords": [
    "support",
    "ai",
    "analytics",
    "saas",
    "customer-service"
  ],
  "author": "SupportIQ Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/supportiq.git"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --project config/build/tsconfig.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:quick": "./scripts/test-quick.sh",
    "test:e2e:fast": "playwright test --workers=4 --timeout=30000 --project=chromium",
    "test:all": "npm run lint && npm run type-check && npm run test && npm run test:e2e:fast",
    "setup": "node scripts/setup/auto-setup.js",
    "setup:full": "npm run setup && node scripts/setup/setup-database.sql && node scripts/setup/setup-stripe-webhook.js",
    "deploy": "npm run build && npm run test:all && vercel --prod",
    "deploy:staging": "npm run build && vercel",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "db:migrate": "npx supabase db push",
    "db:reset": "npx supabase db reset",
    "db:types": "npx supabase gen types typescript --local > lib/types/database.ts",
    "db:studio": "npx supabase studio",
    "stripe:setup": "node config/stripe/create-stripe-products.js",
    "stripe:webhook": "node scripts/setup/setup-stripe-webhook.js",
    "intercom:setup": "node scripts/setup/setup-intercom-quick.js",
    "admin:create": "node scripts/setup/make-admin-script.js",
    "check:env": "node scripts/setup/check-setup.js",
    "check:health": "curl -f http://localhost:3000/api/health || echo 'Server not running'",
    "analyze": "npm run build && npx @next/bundle-analyzer",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "security:audit": "npm audit && npm audit fix",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:check": "npx npm-check-updates",
    "tools": "node scripts/dev-tools.js",
    "monitor:performance": "node scripts/performance-monitor.js",
    "performance": "node scripts/performance-monitor.js",
    "fix:supabase": "node scripts/fix-supabase-urls.js",
    "precommit": "npm run lint && npm run type-check && npm run test"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.4",
    "@sentry/nextjs": "^8.0.0",
    "@stripe/stripe-js": "^5.4.0",
    "@supabase/realtime-js": "^2.12.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.5",
    "@types/crypto-js": "^4.2.2",
    "@types/uuid": "^10.0.0",
    "@vercel/kv": "^3.0.0",
    "autumn-js": "^0.0.100",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "ioredis": "^5.6.1",
    "jose": "^6.0.11",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "openai": "^5.9.0",
    "posthog-js": "^1.100.0",
    "rate-limiter-flexible": "^7.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "recharts": "^3.1.0",
    "redis": "^5.6.0",
    "resend": "^4.0.5",
    "stripe": "^15.12.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.5",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^15.0.0",
    "msw": "^2.10.4",
    "npm-check-updates": "^16.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
